{
    "services": [
        {
            "name": "http_service",
            "target_ip": "http_service",
            "target_port": 80,
            "listen_port": 80,
            "http": true
        }
    ],
    
    "global_config": {
        "keyword": "EH! VOLEVI",
        "verbose": false,
        "dos": {
            "enabled": true,
            "duration": 60,
            "interval": 2
        },
        "max_stored_messages": 10,
        "max_message_size": 65535
    }
}
